name        = Centralbibliotek Theme
description = The default theme for <a href="http://centralbibliotek.dk">Centralbibliotek.dk</a>.
core        = 7.x
base theme  = commons_origins
release     = 7.x-1.x

;; Panels Module layouts
plugins[panels][layouts] = layouts

; Responsive Panels - Standard layout
settings[cb_3col_special]  = 'cb3'

;; Regions
regions[sidebar_first]     = Sidebar first
regions[sidebar_second]    = Sidebar second
regions[content]           = Main Content
regions[highlighted]       = Highlighted
regions[content_aside]     = Aside
regions[secondary_content] = Secondary
regions[tertiary_content]  = Tertiary
regions[footer]            = Footer
regions[header]            = Header
regions[header_two]        = Header Two
regions[menu_bar]          = Menu Bar
regions[help]              = Help
regions[page_top]          = Page top
regions[page_bottom]       = Page bottom


;; Stylesheets

; See each stylesheet for extensive documentation and help.
stylesheets[all][] = css/global.base.css
stylesheets[all][] = css/global.styles.css
stylesheets[all][] = css/cb.base.css
stylesheets[all][] = css/responsive.laptop.css
; Print - uncomment to load basic print styles.
stylesheets[print][] = css/print.css

; Internet Explorer Stylesheets
ie_stylesheets[screen][(lt IE 9)&(!IEMobile 7)][] = css/lt-ie9.css

; Unset Module CSS or other Stylesheets
; unset_css[sites/all/modules/foobar_module/foobar_module.css] = 1

;; Scripts
scripts[] = scripts/cb.js
